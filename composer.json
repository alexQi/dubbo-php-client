{
  "name": "alex-qiu/dubbo-php-client",
  "description": "dubbo php client",
  "type": "library",
  "authors": [
    {
      "name": "alex",
      "email": "alex.qiubo@qq.com",
      "role": "translator"
    },
    {
      "name": "jquick",
      "email": "314127900@qq.com"
    },
    {
      "name": "nick fan",
      "email": "nickfan81@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.0",
    "ext-zookeeper": "*",
    "icecave/chrono": "~1",
    "heyanlong/flax": "^1.0",
    "guzzlehttp/guzzle": "~5.3|~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": {
      "dubbo\\": "src/",
      "dubbo\\invok\\": "src/invok/",
      "dubbo\\protocols\\": "src/invok/protocols/",
      "DubboPhp\\Client\\": "src/DubboPhp/Client/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DubboPhp\\Client\\Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  }
}
